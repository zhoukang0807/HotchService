    {
      "type": "dateFile",
      "filename": "./logs/",
      "pattern": "user/yyyyMMdd.txt",
      "absolute": false,
      "alwaysIncludePattern": true,
      "category": "user"
    }
  ],
  "levels":{ "logInfo": "DEBUG"}
}